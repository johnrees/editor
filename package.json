  "license": "MPL-2.0",
  "devDependencies": {
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/three": "^0.84.36",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "ghpages": "^0.0.10",
    "prettier": "^1.9.2",
    "ts-jest": "^22.0.0",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "@bentobots/three": "^0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rxjs": "^5.5.6",
    "three": "^0.89.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
